{
    "version": 2,
    "builds" : [
        { "src": "/index.html", "use": "@now/static" },
        { "src": "/api/server.js", "use": "@now/node-server" }
    ],
    "routes": [
        { "src": "/api/.*", "dest": "/api/server.js" },
        { "src": "^/static/(.*)", "dest": "/static/$1" },
        { "src": ".*", "dest": "/index.html" }
    ]
}